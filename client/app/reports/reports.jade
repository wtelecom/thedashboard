.row
  .col-xs-12.col-md-12.col-lg-12.animated-panel.zoomIn(style='animation-delay: 0.4s; -webkit-animation-delay: 0.4s;')
    .hpanel
      .panel-body
        .row
          .col-xs-12
            table.table.table-hover.dashboards
              thead
                th Name
                th Time
                th Progress
                th(colspan="2") Actions
              tbody
                tr(ng-repeat='report in reports')
                  td(width="10%") {{ report.name }}
                  td(width="30%") From: {{ report.time.from }} <br/> To: {{ report.time.from }}
                  td
                    .progress
                      .progress-bar.progress-bar-info(role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%;")
                        | 60%
                  td.min(width="1%")
                    a.btn.btn-default.btn-xs(ng-click='showReport(report)')
                      i.glyphicon.glyphicon-eye-open
                  //- td.min(width="1%") 
                  //-   a.btn.btn-danger.btn-xs(ng-click='deleteReport(Report)')
                  //-     i.glyphicon.glyphicon-trash
